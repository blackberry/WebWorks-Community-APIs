    <extension id="webworks.analytics">
        <entryClass>webworks.analytics.AnalyticsExtension</entryClass>
    </extension>
    
    <dependencies>
      <jar path="AnalyticsService_v1.0.jar" />
    </dependencies>
    
    <platforms>
      <platform value="JAVA">
        <target version="default" config="JAVA" />
